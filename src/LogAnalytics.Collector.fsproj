<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Title>LogAnalytics.Collector</Title>
    <Authors>Dave Curylo</Authors>
    <Description>Log Analytics Collector can sign and build messages to send structured logging data to an Azure Log Analytics workspace.</Description>
    <Copyright>Dave Curylo</Copyright>
    <PackageProjectUrl>https://github.com/ninjarobot/LogAnalytics.Collector</PackageProjectUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/ninjarobot/LogAnalytics.Collector</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="LogAnalytics.Collector.fs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="11.0.1" />
  </ItemGroup>
</Project>
